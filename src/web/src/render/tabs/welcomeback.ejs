<h1 class="text-3xl font-black">Welcome back, <span id="u-token">...</span>!</h1>
<p class="mt-1 text-white text-opacity-60 text-[#8b9294]">Use the sidebar to nagivate.</p>
<script type="text/javascript" id="invisible">
    fetch("https://discord.com/api/users/@me", {
        headers: {
            authorization: "<%= token %>"
        }
    })
        .then(res => res.json())
        .then(data => {
            document.querySelector("#u-token").textContent = `${data.username}`
        }).catch(console.error);
</script>